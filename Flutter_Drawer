// main.dart

import 'package:flutter/material.dart'; 
import 'travell.dart'; 
void main() { 
 runApp(const Travell()); 
} 

// travell.dart

import 'package:flutter/material.dart'; 
class Travell extends StatelessWidget { 
 const Travell({super.key}); 
 @override 
 Widget build(BuildContext context) { 
 return const MaterialApp( 
 debugShowCheckedModeBanner: false, 
 home: TravellExample(), 
 ); 
 } 
} 
class TravellExample extends StatelessWidget { 
 const TravellExample({super.key}); 
 @override 
 Widget build(BuildContext context) { 
 return Scaffold( 
 appBar: AppBar( 
 title: const Text("Travell App"), 
 ), 
 drawer: Drawer(
 child: ListView( 
 padding: EdgeInsets.zero, 
 children: [ 
 DrawerHeader( 
 decoration: const BoxDecoration(color: Colors.blue),  child: Column( 
 mainAxisAlignment: MainAxisAlignment.center,  children: const [ 
 CircleAvatar( 
 radius: 30, 
 backgroundColor: Colors.white, 
 child: Icon(Icons.person, size: 40),  ), 
 SizedBox(height: 10), 
